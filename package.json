{
  "$schema": "https://json.schemastore.org/package",
  "name": "kinopoisk-with-fetch-dev-api",
  "version": "1.0.0",
  "description": "Simple web-client for kinopoisk.dev api with javascript fetch api by Junproger",
  "keywords": [
    "openmovie api",
    "kinopoisk.dev",
    "kinopoisk api",
    "web-client",
    "fetch api"
  ],
  "homepage": "https://github.com/junproger/kinopoisk-with-fetch-dev-api",
  "repository": "git+https://github.com/junproger/kinopoisk-with-fetch-dev-api.git",
  "author": "Junproger <junproger@gmail.com> (https://github.com/junproger)",
  "private": true,
  "license": "ISC",
  "source": "./source/index.html",
  "targets": {
    "default": {
      "distDir": "./build"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "typing": "tsc --noEmit",
    "checkup": "prettier --check source/",
    "format": "prettier --write source/",
    "start": "parcel serve --open",
    "build": "parcel build --public-url ./",
    "watch": "parcel watch ./source/index.html",
    "linter": "eslint \"./source/**/*.{js,jsx,ts,tsx}\"",
    "linter:fix": "eslint \"./source/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@parcel/packager-raw-url": "2.9.3",
    "@parcel/transformer-webmanifest": "2.9.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "parcel": "^2.9.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  }
}
